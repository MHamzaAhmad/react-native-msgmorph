{
  "name": "@msgmorph/react-native",
  "version": "0.1.0",
  "description": "MsgMorph React Native SDK - Native feedback widget and live chat for React Native apps",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint ./src",
    "clean": "rimraf dist"
  },
  "keywords": [
    "react-native",
    "msgmorph",
    "feedback",
    "widget",
    "live-chat",
    "customer-support"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msgmorph/msgmorph.git",
    "directory": "packages/sdks/msgmorph_rn"
  },
  "author": "MsgMorph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msgmorph/msgmorph/issues"
  },
  "homepage": "https://msgmorph.com",
  "peerDependencies": {
    "react": "^17.0.0",
    "react-native": "^0.83.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "react-native-safe-area-context": "^4.0.0"
  },
  "dependencies": {
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^1.23.0",
    "@types/react": "~19.2.7",
    "@types/react-native": "^0.72.0",
    "react": "19.2.3",
    "react-native": "^0.83.0",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}